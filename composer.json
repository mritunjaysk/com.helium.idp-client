{
    "name": "helium/idp-client",
    "description": "A package for interacting with the Helium IDP service as an application client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nick Rupert",
            "email": "nick@heliumservices.com"
        }
    ],
    "require": {
        "jenssegers/model": "^1.3|^1.5",
	"illuminate/support": "^7.9|^8.2",
        "guzzlehttp/guzzle": "^6.5|^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^5.2|^6.0"
    },
	"autoload": {
		"psr-4": {
			"Helium\\IdpClient\\": "src/",
			"Helium\\IdpClient\\Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Helium\\IdpClient\\Providers\\IdpServiceProvider"
			]
		}
	}
}
